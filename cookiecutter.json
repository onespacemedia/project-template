{
    "project_name": "{{ cookiecutter.project_name|default('Example Project') }}",
    "domain_name": "{{ cookiecutter.domain_name|default('example.com') }}",
    "staging_subdomain": "{{ cookiecutter.staging_subdomain|default('example') }}",
    "repo_name": "{{ cookiecutter.project_name|lower|replace(' ', '-') }}",
    "package_name": "{{ cookiecutter.repo_name|replace('-', '_') }}",
    "faqs": "no",
    "careers": "no",
    "people": "no",
    "redirects": "yes",
    "news": "yes",
    "sections": "yes",
    "footer": "yes",
    "geoip": "no",
    "email_password": "{{ cookiecutter.email_password }}",
    "google_plus_key": "{{ cookiecutter.google_plus_key }}",
    "google_plus_secret": "{{ cookiecutter.google_plus_secret }}",
    "opbeat_app_id": "{{ cookiecutter.opbeat_app_id }}",
    "opbeat_secret_token": "{{ cookiecutter.opbeat_secret_token }}",
