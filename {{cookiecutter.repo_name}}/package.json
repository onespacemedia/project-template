  "scripts": {
    "dev": "gulp",
    "build": "gulp production",
    "lint:css": "stylelint \"{{cookiecutter.package_name}}/assets/**/*.css\"",
    "lint:js": "eslint --ext .js,.vue {{cookiecutter.package_name}}/assets/js/**/*.js",
    "lint:iSort": "isort --check-only --diff --quiet --skip-glob=.venv --skip-glob=node_modules --skip-glob=*/migrations/*",
    "lint:py": "pylint {{cookiecutter.package_name}}/ --load-plugins pylint_django --ignore=migrations,tests -d missing-docstring,invalid-name,no-init,too-many-ancestors,no-member,line-too-long,attribute-defined-outside-init,too-few-public-methods,no-self-use,unused-argument,protected-access,locally-disabled,duplicate-code,fixme --reports=n",
    "git:add": "git add",
    "lint-staged": "lint-staged"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "es2015",
      "es2016",
      "stage-0"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.css": [
      "lint:css",
      "git:add"
    ],
    "*.@(js)": [
      "lint:js",
      "git:add"
    ],
    "*.py": [
      "lint:iSort",
      "lint:py",
      "git:add"
    ]
  }
}
