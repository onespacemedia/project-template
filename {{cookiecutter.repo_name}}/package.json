    "webpack-dev-server": "^3.9.0"
  },
  "scripts": {
    "precommit": "if test -e /usr/local/bin/virtualenvwrapper.sh >/dev/null 2>&1; then export VIRTUALENVWRAPPER_PYTHON=/usr/local/bin/python && source /usr/local/bin/virtualenvwrapper.sh && workon \"${PWD##*/}\"; else . .venv/bin/activate; fi; lint-staged",
    "build": "webpack -p --display-error-details",
    "dev": "webpack-dev-server --host 0.0.0.0 --watch-content-base",
    "lint:css": "stylelint \"{{cookiecutter.package_name}}/{assets/**,apps/*/assets/css/**}/*.css\"",
    "lint:js": "eslint --ext .js,.vue {{cookiecutter.package_name}}/assets/js/{**/*.js,*.js}",
    "lint:python": "pylint {{cookiecutter.package_name}}/",
    "lint:isort": "isort --check-only --diff --quiet"
  },
  "lint-staged": {
    "{{cookiecutter.package_name}}/assets/js/{**/*.js,*.js}": [
      "prettier-standard --print-width 120",
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "git add"
    ],
    "{{cookiecutter.package_name}}/{assets/**,apps/*/assets/css}/*.css": [
      "prettier-standard --print-width 120",
      "stylelint --fix",
      "git add"
    ],
    "**/!(migrations|tests)/*.py": [
      "isort --skip-glob=.venv --skip-glob=node_modules --skip-glob=*/migrations/*",
      "pylint --load-plugins pylint_django,pylint.extensions.mccabe -d missing-docstring,invalid-name,no-init,too-many-ancestors,no-member,line-too-long,attribute-defined-outside-init,too-few-public-methods,no-self-use,unused-argument,protected-access,locally-disabled,duplicate-code,fixme --reports=n",
      "git add"
    ]
  },
  "browserslist": [
    "Android >= 4.4.4",
    "Chrome >= 49",
    "Edge >= 14",
    "Firefox >= 52",
    "ie >= 11",
    "iOS >= 9.3",
    "Opera >= 43",
    "Safari >= 9.1"
  ]
}
